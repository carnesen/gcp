{
  "name": "@carnesen/google-cloud",
  "version": "0.0.0",
  "description": "A library for deploying websites to Google Cloud Platform",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "engines": {
    "node": ">=8.0.0"
  },
  "scripts": {
    "build": "rimraf lib && tsc --project tsconfig.build.json",
    "build:watch": "npm run build -- --watch",
    "example": "ts-node src/example.ts",
    "lint": "tslint --project tsconfig.build.json",
    "lint:fix": "npm run lint -- --fix",
    "test": "npm run lint && npm run test:unit && npm run build",
    "test:unit": "cross-env NODE_ENV=test jest src",
    "test:unit:coverage": "npm run test:unit -- --watch --coverage",
    "test:unit:watch": "npm run test:unit -- --watch",
    "preversion": "npm test",
    "postversion": "npm publish"
  },
  "dependencies": {
    "@google-cloud/dns": "0.9.1",
    "@types/js-yaml": "3.12.0",
    "js-yaml": "3.12.1"
  },
  "devDependencies": {
    "@carnesen/meme-me": "github:carnesen/meme-me",
    "@carnesen/redirector": "0.0.0",
    "@carnesen/run-and-exit": "0.2.0",
    "@carnesen/tsconfig": "0.1.0",
    "@carnesen/tslint-config": "0.1.2",
    "@carnesen/www": "0.2.0",
    "@types/jest": "23.3.13",
    "@types/node": "^10.12.21",
    "cross-env": "5.2.0",
    "jest": "24.0.0",
    "rimraf": "2.6.3",
    "ts-jest": "23.10.5",
    "ts-node": "8.0.2",
    "tslint": "5.12.1",
    "typescript": "3.3.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/carnesen/google-cloud.git"
  },
  "author": "Chris Arnesen",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/carnesen/google-cloud/issues"
  },
  "homepage": "https://github.com/carnesen/google-cloud",
  "files": [
    "src",
    "lib"
  ]
}
