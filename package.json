{
  "name": "@carnesen/google-cloud",
  "version": "0.0.0",
  "description": "A library for deploying websites to Google Cloud Platform",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "engines": {
    "node": ">=8.0.0"
  },
  "scripts": {
    "build": "rimraf lib && tsc --project tsconfig.build.json",
    "example": "ts-node src/example.ts",
    "lint": "tslint --project tsconfig.build.json",
    "lint:fix": "npm run lint -- --fix",
    "test": "npm run lint && npm run unit-test && npm run build",
    "unit-test": "cross-env NODE_ENV=test jest src"
  },
  "dependencies": {
    "@google-cloud/dns": "2.0.0",
    "@types/js-yaml": "3.12.4",
    "js-yaml": "3.14.0"
  },
  "devDependencies": {
    "@carnesen/meme-me": "github:carnesen/meme-me",
    "@carnesen/redirector": "0.0.0",
    "@carnesen/run-and-exit": "0.2.1",
    "@carnesen/tsconfig": "0.2.0",
    "@carnesen/tslint-config": "0.2.0",
    "@carnesen/www": "0.2.0",
    "@types/jest": "25.2.3",
    "@types/node": "^14.0.5",
    "cross-env": "7.0.2",
    "jest": "26.0.1",
    "rimraf": "3.0.2",
    "ts-jest": "26.0.0",
    "ts-node": "8.10.1",
    "tslint": "6.1.2",
    "typescript": "3.9.3"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/carnesen/google-cloud.git"
  },
  "author": "Chris Arnesen",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/carnesen/google-cloud/issues"
  },
  "homepage": "https://github.com/carnesen/google-cloud",
  "files": [
    "src",
    "lib"
  ]
}
